{
	"name": "DelimitedText_LogAnalytics_Source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLGen2_LinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ContainerName": {
				"type": "string",
				"defaultValue": "azureactivity"
			},
			"FolderName": {
				"type": "string",
				"defaultValue": "generallog"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().FolderName, formatDateTime(pipeline().TriggerTime,'MM/dd/yyyy'))",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": "|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}